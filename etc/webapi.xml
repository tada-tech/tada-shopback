<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <!-- Cms Page -->
    <route url="/V1/shopback/order" method="GET">
        <service class="Tada\Shopback\Api\FakeShopbackRepositoryInterface" method="createOrder"/>
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
    <route url="/V1/shopback/validate" method="GET">
        <service class="Tada\Shopback\Api\FakeShopbackRepositoryInterface" method="validateOrder"/>
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
</routes>
